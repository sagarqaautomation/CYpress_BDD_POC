{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:chrome:smoke": "cypress-tags run --browser chrome --headed \"TAGS=@smoke\"",
    "Report": "node cucumber-html-report.js",
    "cy:run:chrome:smoke:Report": "cypress-tags run --browser chrome --headed \"TAGS=@smoke\" Report"
  },
  "author": "sagar",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.5.3",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^1.19.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "save-dev": "^0.0.1-security"
  }
}
